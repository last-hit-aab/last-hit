{
	"name": "last-hit-replayer",
	"version": "0.8.4",
	"description": "Replayer for last-hit recorded data.",
	"main": "dist/index.js",
	"types": "index",
	"bin": {
		"last-hit-replayer": "./bin/entry.js"
	},
	"scripts": {
		"start": "gulp && node bin/entry.js",
		"test": "last-hit-replayer",
		"deploy": "gulp && npm publish --access public",
		"deploy:dev": "gulp && npm publish --tag dev",
		"compile": "gulp",
		"update:dev": "yarn add last-hit-extensions@dev"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/last-hit-aab/last-hit.git"
	},
	"keywords": [
		"test",
		"web",
		"automation"
	],
	"author": "last-hit-team",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/last-hit-aab/last-hit/issues"
	},
	"homepage": "https://www.last-hit.com",
	"dependencies": {
		"@types/atob": "^2.1.2",
		"@types/clone": "^0.1.30",
		"@types/cross-spawn": "^6.0.1",
		"@types/jsonfile": "^5.0.0",
		"@types/mkdirp": "^0.5.2",
		"@types/pngjs": "^3.4.0",
		"@types/puppeteer": "^1.20.2",
		"@types/uuid": "^3.4.6",
		"@types/yargs": "^13.0.3",
		"atob": "^2.1.2",
		"clone": "^2.1.2",
